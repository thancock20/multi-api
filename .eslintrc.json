{
  "extends": ["airbnb-base", "prettier", "plugin:react/recommended"],
  "plugins": ["react", "jest"],
  "env": {
    "jest/globals": true,
    "browser": true
  },
  "rules": {
    "react/react-in-jsx-scope": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
